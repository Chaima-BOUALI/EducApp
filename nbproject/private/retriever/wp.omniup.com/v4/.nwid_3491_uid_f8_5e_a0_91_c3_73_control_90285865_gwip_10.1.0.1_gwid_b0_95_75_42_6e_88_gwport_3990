<script>
    /**
     * Login
     */
     function validate_login_form() {

        var age = $('#age').val();
        var gender = $('input[name=gender]:checked').val();
        //$("#myform input[type='radio']:checked").val();

        if (typeof age === "undefined" || !age)
        {
            alert("Select your age range");
            return false;
        }
        if (typeof gender === "undefined" || !gender)
        {
            alert("Select your gender");
            return false;
        }
            
    	$.ajax({
           type: "POST",
           url: 'https://wp.omniup.com/v4/?nwid=3491&uid=f8%3A5e%3Aa0%3A91%3Ac3%3A73&control=90285865&gwip=10.1.0.1&gwid=b0%3A95%3A75%3A42%3A6e%3A88&gwport=3990',
           data: $("#idForm").serialize(), // serializes the form's elements.
           success: function(data)
           {
               location.reload(true);
           }
         });
        $("button[type='submit']").attr('disabled', 'disabled').text("Processing...");
        return false;
    }
    

    
</script>
